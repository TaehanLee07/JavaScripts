<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script>
        let theText = "Sungil Information Highschool"
        document.write(theText + "<br>")
        document.write(theText.bold() + "<br>")
        document.write(theText.link("https://www.sungil-i.kr") + "<br>")
        document.write(theText.length + "<br>")
        document.write(theText.toLowerCase() + "<br>")
        document.write(theText.toUpperCase() + "<br>")
        document.write(theText.indexOf("H") + "<br>")
        document.write(theText.lastIndexOf("n") + "<br>")   // 마지막으로 나오는 n 의 위치
        document.write(theText.charAt(5) + "<br>")
        document.write(theText.substring(7, 18) + "<br>")   // 7 ~ 18
        document.write(theText.substr(7) + "<br>")   // 7번지 부터 18글자
        document.write(theText.replace("High", "Middle") + "<br>")
        document.write(theText.slice(7, 9) + "<br>") // 문자열의 일부를 추추랗여 새로운 문자열 변환
        document.write(theText.concat(" Fighting") + "<br>")
        document.write(theText.split(" ") + "<br>")




    </script>
</head>

<body>

</body>

</html>
